{
  "name": "big-div-energy",
  "version": "1.0.0",
  "description": "A responsive layout component library for React.",
  "author": "VirtuaBoza",
  "license": "MIT",
  "repository": "VirtuaBoza/big-div-energy",
  "keywords": [
    "react",
    "layout",
    "components"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc && babel src -d dist --extensions \".ts,.tsx\"",
    "deploy-storybook": "storybook-to-ghpages",
    "prepare": "husky install",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "release": "standard-version"
  },
  "dependencies": {},
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/react": "^11.4.1",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/storybook-deployer": "^2.8.10",
    "@types/react": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "babel-loader": "^8.2.2",
    "csstype": "3.0.9",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "4.2.1-alpha-c3a19e5af-20211014",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "standard-version": "^9.3.1",
    "typescript": "^4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
