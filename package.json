{
  "name": "big-div-energy",
  "version": "1.0.0",
  "description": "A responsive layout component library for React.",
  "author": "VirtuaBoza",
  "license": "MIT",
  "repository": "VirtuaBoza/big-div-energy",
  "keywords": [
    "react",
    "layout",
    "components"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && babel src -d dist --extensions \".ts,.tsx\"",
    "deploy-storybook": "storybook-to-ghpages",
    "prepare": "husky install"
  },
  "dependencies": {},
  "peerDependencies": {
    "@emotion/react": "^11.4.1",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@emotion/babel-plugin": "^11.3.0",
    "@storybook/storybook-deployer": "^2.8.10",
    "@types/react": "^17.0.30",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.26.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/*": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
